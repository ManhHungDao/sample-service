    "@nestjs/swagger": "3.0.2",
    "@nestjs/testing": "6.11.11",
    "@types/uuid": "3.4.5",
    "amqp-connection-manager": "3.0.0",
    "amqplib": "^0.8.0",
    "body-parser": "1.19.0",
    "cache-manager": "2.9.1",
    "cache-manager-redis": "0.6.0",
    "cache-manager-redis-store": "1.5.0",
    "class-transformer": "^0.5.1",
